{
  "name": "@zikeji/hypixel",
  "version": "3.1.1",
  "type": "module",
  "description": "With thorough IntelliSense support & 100% test coverage, this is an unopinionated async/await API wrapper for Hypixel's Public API. It is developed in TypeScript complete with documentation, typed interfaces for all API responses (and an OpenAPI 3.0 schema!), built-in rate-limit handling, flexible cache support, helper functions, and support for undocumented endpoints.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "commit": "cz",
    "lint": "eslint --ext ts src --fix",
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc -p .",
    "update": "yarn upgrade-interactive",
    "types:openapi": "dtsgen --out ./src/types/api.ts --target ES2015 ./openapi.yaml && node ./scripts/process_api_output.js && eslint --ext ts ./src/types/api.ts --fix",
    "watch": "yarn clean && tsc -p . -w",
    "test": "nodenv --exec npx -- mocha -r ts-node/esm tests/**/*.test.ts",
    "test:helpers": "nodenv --exec npx -- mocha -r ts-node/esm tests/helpers.test.ts",
    "test:ci": "mocha -r ts-node/esm tests/**/*.test.ts",
    "test:lint": "eslint --ext ts src",
    "coverage": "nyc npm t",
    "coverage:ci": "nyc yarn test:ci",
    "docs:dev": "yarn docs:clean:ts-api && vuepress dev docs",
    "docs:clean:ts-api": "rm -rf docs/ts-api",
    "docs:build": "yarn docs:clean:ts-api && vuepress build docs",
    "test:build": "yarn test:lint && yarn build && yarn test && yarn coverage && yarn docs",
    "prepack": "yarn build",
    "format": "prettier --write src/*"
  },
  "homepage": "https://node-hypixel.zikeji.com/",
  "author": "Zikeji <me@zikeji.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zikeji/node-hypixel/issues"
  },
  "repository": {
    "url": "git@github.com:zikeji/node-hypixel.git"
  },
  "keywords": [
    "api",
    "api-wrapper",
    "hypixel",
    "skyblock",
    "minecraft",
    "hypixel skyblock",
    "minecraft hypixel",
    "hypixel minecraft",
    "hypixel minecraft skyblock",
    "hypixel api",
    "skyblock api",
    "minecraft api"
  ],
  "files": [
    "dist/",
    "src/",
    "!dist/**/*.map",
    "!dist/tsconfig.tsbuildinfo",
    "!src/**/*.deno.ts{,x}",
    "!docs/"
  ],
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@mdi/svg": "^7.0.96",
    "@mr-hope/vuepress-plugin-last-update": "^1.11.0",
    "@mr-hope/vuepress-types": "^1.30.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/debug": "^4.1.7",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.14",
    "@types/yargs-parser": "^21.0.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.2.5",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "dtsgenerator": "^3.16.0",
    "eslint": "^8.23.0",
    "eslint-config-neon": "^0.1.32",
    "esm": "^3.2.25",
    "mocha": "^10.0.0",
    "node-env-run": "^4.0.2",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "prismarine-nbt": "^2.2.1",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remarkable": "^2.0.1",
    "semantic-release": "^19.0.5",
    "stylus-svg": "^1.3.1",
    "swagger-ui": "^4.14.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.13",
    "typedoc-plugin-markdown": "^3.13.5",
    "typescript": "^4.8.2",
    "vuepress": "^1.9.7",
    "vuepress-plugin-typedoc": "^0.11.1",
    "vuepress-theme-hope": "^1.30.0",
    "yaml-loader": "^0.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@sapphire/async-queue": "^1.5.0",
    "@sapphire/ratelimits": "^2.4.4",
    "undici": "^5.10.0"
  }
}
